{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@planner/ideias": {
      "projectType": "library",
      "root": "projects/planner/ideias",
      "sourceRoot": "projects/planner/ideias/src",
      "prefix": "planner",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/planner/ideias/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/planner/ideias/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/planner/ideias/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/planner/ideias/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
